  "main": "index.js",
  "license": "Apache-2.0",
  "devDependencies": {
    "aws-sdk-mock": "^5.1.0",
    "jest": "^25.1.0",
    "serverless": "^1.72.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-step-functions": "^2.19.0",
    "sinon": "^9.0.2",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "aws-sdk": "^2.610.0",
    "moment": "^2.26.0"
  },
  "scripts": {
